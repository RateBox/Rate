{
  "kind": "collectionType",
  "collectionName": "listing_types",
  "info": {
    "singularName": "listing-type",
    "pluralName": "listing-types",
    "displayName": "Listing Type",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "Name": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "Slug": {
      "type": "uid",
      "targetField": "Name"
    },
    "Description": {
      "type": "blocks",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "isActive": {
      "type": "boolean",
      "default": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "Categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category.category",
      "mappedBy": "ListingType"
    },
    "Criteria": {
      "type": "component",
      "repeatable": true,
      "component": "rating.criterion",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "allowComment": {
      "type": "boolean",
      "default": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "allowRating": {
      "type": "boolean",
      "default": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "ItemGroup": {
      "type": "json",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "ReviewGroup": {
      "type": "json"
    },
    "IconSet": {
      "type": "enumeration",
      "enum": [
        "Classic",
        "Warning",
        "Thumb",
        "Minimal"
      ],
      "default": "Classic"
    }
  }
}
